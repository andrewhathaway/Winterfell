{
  "name": "winterfell",
  "version": "1.1.0",
  "description": "Generate complex, validated and extendable JSON-based forms in React",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build": "babel src --out-dir dist && webpack",
    "eslint": "eslint src --ext .jsx --ext .js",
    "eslint-fix": "eslint src --ext .jsx --ext .js --fix",
    "eslint-watch": "esw src -w --color --ext .jsx --ext .js",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "winterfell",
    "form",
    "forms",
    "engine",
    "react",
    "validation",
    "complex",
    "extendable",
    "customisable",
    "conditional",
    "questions",
    "react-component"
  ],
  "author": "Andrew Hathaway <andrew@andrewhathaway.net>",
  "repository": {
    "type": "git",
    "url": "git://github.com/andrewhathaway/winterfell.git"
  },
  "license": "MIT",
  "dependencies": {
    "babelify": "^6.3.0",
    "keycodez": "^1.0.0",
    "lodash": "^3.10.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "validator": "^4.0.5"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "browserify": "^11.0.1",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "11.1.0",
    "eslint-friendly-formatter": "2.0.6",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.6.0",
    "eslint-watch": "^3.0.1",
    "gulp": "^3.9.0",
    "gulp-uglify": "^1.2.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "rimraf": "^2.4.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.12.1"
  }
}
